<!DOCTYPE html>
<html lang="en">
<head>
  <title></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/css/table.css">
</head>
<body>
  <%- include("./partials/error"); %>
<!--====form section start====-->
    <div class="table-data">
    <table border="1" >
        <tr>
            <th>S.N</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            <th>Date of Birth</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
        
        <%
        if(Customer.length!=0){
        var i=1;
        Customer.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.email %></td>
            <td><%=data.firstName %></td>
            <td><%=data.lastName %></td>
            <td><%=data.phone %></td>
            <td><%=data.DateOfBirth %></td>
            <td><a href="/users/userEdit/<%=data.id%>">Edit</a></td>
            <td><a href="/users/userDelete/<%=data.id%>">Delete</a></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    <a href="/users/dashboard" class="btn btn-secondary">Return to Dashboard</a>
    </div>
  
</body>
<br>

</html>